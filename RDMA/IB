      bool is_dead();
      QueuePair* create_queue_pair();
      ibv_srq* create_shared_receive_queue();
      int post_chunk();
      int post_channel_cluster();
      int get_tx_buffers();
      CompletionChannel* create_comp_channel();
      CompletionQueue* create_comp_queue();
      uint8_t get_ib_physical_port();
