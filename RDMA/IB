class port
      Port();
      uint16_t get_lid();
      ibv_gid get_gid();
      uint8_t get_port_num();
      ibv_port_attr* get_port_attr();
      
class Device
      Device();
      ~Device();
      const char* get_name();
      uint16_t get_lid();
      ibv_gid get_gid();
      void binding_port();
      
class DeviceList
      DeviceList();
      ~DeviceList();
      Device* get_device();
      
class ProtectionDomain
      ProtectionDomain();
      ~ProtectionDomain();
      
class Infiniband
      Infiniband();
      ~Infiniband();
      void wire_gid_to_gid();
      void gid_to_wire_gid();
      
class MemoryManager
      MemoryManager();
      ~MemoryManager();
      void malloc_huge_pages();
      void free_huge_pages();
      void register_rx_tx();
      void return_tx();
      int get_send_buffers();
      int get_channel_buffers();
      int is_tx_chunk();
      int is_rx_chunk();
      
      
class Chunk
      Chunk();
      ~Chunk();
      void set_offset();
      size_t get_offset();
      void set_bound();
      void prepare_read();
      uint32_t get_bound();
      size_t read();
      size_t write();
      bool full();
      bool over();
      void clear();
      void post_srq();
      void set_owner();
      uint64_t get_owner();
      
class Cluster
      Cluster();
      ~Cluster();
      int add();
      void take_back();
      int get_buffers();
      
class CompletionChannel
      CompletionChannel();
      ~CompletionChannel();
      int init();
      bool get_cq_event();
      int get_fd();
      ibv_comp_channel* get_channel();
      void bind_cq();
      void ack_events();
      
class CompletionQueue
      CompletionQueue();
      ~CompletionQueue();
      int init();
      int poll_cq();
      ibv_cq* get_cq();
      int rearm_notify();
      CompletionChannel* get_cc();
      
class QueuePair
      QueuePair();
